&sdcp_instrument_list
! First draft based on EMBL-HH P14
calibration_name= 'Initial'
beamline_name= 'PX2'
coord_sys_name= 'SOLEIL-PX2'
nominal_beam_dir= 0.0  0.0  1.0

beam_sd_deg= 0.03 0.03
! lambd_sd is just a guess at the moment.
lambda_sd= 0.0002
pol_plane_n= 0.0 1.0 0.0
pol_frac= 0.99
d_sensor= 0.45
mu_sensor_def = -1.0
mu_air_def = -1.0
min_zeta= 0.15

! From CF data (see top comment)
det_name= "EIGER9M"
det_x_axis= 1.0  0.0  0.0
det_y_axis= 0.0  1.0  0.0
det_qx= 0.075000
det_qy= 0.075000

det_nx= 3110
det_ny= 3269
! Input values from 20170705
! http://confluence.globalphasing.com/display/autoPROC/2017/07/26/Vincent+Chaptal+-+Proxima2+dataset+201707
det_org_x= 1483.107
det_org_y= 1527.255
! Reference distance - no effect on actual experiments
! for potential refinement of det_gonio_axis_dirs
det_org_dist= 450.0

det_gonio_axis_dirs=  0.0  0.0  1.0
 ! Actual value is irrelevant, since axis is (nominally) parallel to beam
 ! and we don't refine its direction
det_gonio_axis_datum_settings= 500
det_gonio_axis_limits= 195 1000
det_gonio_axis_names= 'Distance'
det_gonio_axis_types= 'Translation'
det_gonio_axis_calibratable = 'False'
gain= 1.0
background= 8.0
det_filename_suffix= '.cbf'

gonio_name= "MiniKappa"
gonio_axis_dirs= 0.0 -1.0 0.0,
   0.195169, -0.9135452905193042, 0.356853
   0.0 -1.0 0.0
gonio_axis_datum_settings= 0 0 0
gonio_axis_limits= , , -8, 256, , ,
gonio_axis_names= 'phi' 'kappa' 'kappa_phi'
gonio_axis_scannable= 'True' 'False' 'False'
gonio_axis_calibratable= 'False' 'True' 'True'

! Nominal centring axis directions with correct signs
! Try higher precision as advised by Claus
! gonio_centring_axis_dirs=  0.0,  -1.0,  0.0,
!                            -0.959683,  0, -0.281085,
!                           0.281085,  0, -0.959683
gonio_centring_axis_dirs =  0.000000000000000E+000,  -1.000000000000000     ,  0.000000000000000E+000
                           -0.959683000000000     ,   0.000000000000000E+000, -0.28108457714894284110
                            0.28108457714894284110,   0.000000000000000E+000, -0.959683000000000
! Actual limits on centring axes
gonio_centring_axis_limits= -116.998, 10.5, -2.396, 2.4, -2.3988, 2.4
gonio_centring_axis_names= 'phiy', 'sampy', 'sampx'


!beamstop_name= 'BEAMSTOPV1'
!beamstop_param_names= 'BEAM_STOP_RADIUS' 'BEAM_STOP_S_LENGTH' 'BEAM_STOP_S_DISTANCE'
!beamstop_param_vals=    1.0               -120.0                60.0

! Taken from http://xds.mpimf-heidelberg.mpg.de/html_doc/INPUT_templates/XDS-PILATUS.INP
! xds_DETECTOR= EIGER
! xds_MINIMUM_VALID_PIXEL_VALUE= 0
! xds_TRUSTED_REGION= 0.0 1.41
! xds_OVERLOAD= 1000000

/

